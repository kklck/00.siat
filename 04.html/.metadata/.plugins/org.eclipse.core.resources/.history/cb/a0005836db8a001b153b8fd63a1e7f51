<%
	String s1 = request.getParameter("num1");
	String s2 = request.getParameter("num1");
	String opr = request.getParameter("num1");
	
	int num1 = new Integer(s1);
	int num2 = new Integer(s2);
	
	int result = 0;
	
	if("+".equals(opr)){
		out.println(num1+num2);
	}else if("-".equals(opr)){
		out.println(num1-num2);
	}else if("*".equals(opr)){
		out.println(num1*num2);
	}else if("/".equals(opr)){
		out.println(num1/num2);
	}
%>